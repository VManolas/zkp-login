# Environment variables for zkSync Era ZKP Login dApp

# Private key for deployment (NEVER commit this to version control)
PRIVATE_KEY=your_private_key_here

# RPC URLs
RPC_URL=https://sepolia.era.zksync.dev
ETH_RPC_URL=https://sepolia.infura.io/v3/your_infura_key

# Contract addresses (will be updated after deployment)
VERIFIER_ADDRESS=0x0000000000000000000000000000000000000000
LOGIN_AUTH_ADDRESS=0x0000000000000000000000000000000000000000

# Network configuration
NETWORK=zkSyncEraTestnet
CHAIN_ID=300

# Frontend configuration
REACT_APP_NETWORK=zkSyncEraTestnet
REACT_APP_CHAIN_ID=300
REACT_APP_RPC_URL=https://sepolia.era.zksync.dev

# Circuit configuration
CIRCUIT_WASM_PATH=/circuits/login_auth.wasm
CIRCUIT_ZKEY_PATH=/circuits/login_auth_final.zkey
CIRCUIT_VERIFICATION_KEY_PATH=/circuits/verification_key.json

# Testing configuration
TEST_PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
TEST_RPC_URL=http://localhost:3050

# Development configuration
NODE_ENV=development
DEBUG=true

